{
    "name": "laravel-enso/roles",
    "description": "Role Manager dependency for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "roles",
        "role-manager",
        "laravel-roles"
    ],
    "homepage": "https://github.com/laravel-enso/Roles",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "laravel-enso/core": "4.6.*",
        "laravel-enso/enums": "1.1.*",
        "laravel-enso/forms": "3.3.*",
        "laravel-enso/helpers": "1.14.*",
        "laravel-enso/menus": "3.3.*",
        "laravel-enso/permissions": "3.3.*",
        "laravel-enso/select": "3.2.*",
        "laravel-enso/migrator": "1.0.*",
        "laravel-enso/tables": "3.1.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Roles\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Roles\\AppServiceProvider",
                "LaravelEnso\\Roles\\EnumServiceProvider"
            ],
            "aliases": {}
        }
    }
}
